pipeline{
    agent any

    stages{
        stage('Build') {
            steps {
                echo 'build'
                 sh 'mvn package'
            }

        }
        stage('push') {
            steps {
                echo 'testing'

                sh "aws s3 cp target/bermtec-0.0.1.war s3://elasticbeanstalk-us-east-2-597671908319"
            }

        }
        stage('Quality Gate-Sonar Qube') {
            steps {
                echo 'sonarcube'
            }

        }
        stage('push to artifactory') {
            steps {
                echo 'push to artifactory'
            }

        }
        stage('Deploy in QA') {
            steps {
                echo 'deploy to qa'
            }

        }
        stage('Deploy to UAT') {
            steps {
                echo 'uat'
            }

        }
        stage('Deploying to staging') {
            steps {
                echo 'staging'
            }

        }
        stage('Deploy to prod') {
            steps {
                echo 'prod'
            }

        }
    }
        post {
            failure{
            echo 'failed'
        }
        success{
            echo 'success'
        }
        aborted{
            echo 'aborted'
        }
        always{
            echo 'always'
        }

        }
    
}